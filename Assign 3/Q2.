#include <stdio.h>
#include <stack>
using namespace std;

int main() {
  stack<char> st;
  string str;

  cout << "Enter a string: ";
  getline(cin, str);

  for (int i = 0; i < str.length(); i++) {
    st.push(str[i]);
  }

  cout << "Reversed String: ";
  while (!st.empty()) {
    cout << st.top();
    st.pop();
  }
}
